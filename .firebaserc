{
  "projects": {
    "prod": "devshare-1",
    "stg": "devshare-stg",
    "v0.3.3": "devshare-int",
    "int": "devshare-int",
    "master": "devshare-int",
    "default": "devshare-int"
  },
  "ci": {
    "mapEnv": {
      "TRAVIS_BRANCH": "travis.branch"
    },
    "createConfig": {
      "v0.3.3": {
        "version": "${npm_package_version || ''}",
        "env": "prod",
        "firebase": {
          "apiKey": "${STAGE_FIREBASE_WEBAPIKEY}",
          "authDomain": "devshare-int.firebaseapp.com",
          "databaseURL": "https://devshare-int.firebaseio.com",
          "projectId": "devshare-int",
          "storageBucket": "devshare-int.appspot.com",
          "messagingSenderId": "499842460400"
        },
        "sentryDsn": "${SENTRY_DSN}",
        "gaTrackingId": "${GA_TRACKINGID}"
      },
      "stage": {
        "version": "${npm_package_version || ''}",
        "env": "stage",
        "firebase": {
          "apiKey": "${STAGE_FIREBASE_WEBAPIKEY}",
          "authDomain": "devshare-stage.firebaseapp.com",
          "databaseURL": "https://devshare-stage.firebaseio.com",
          "projectId": "devshare-stage",
          "storageBucket": "devshare-stage.appspot.com",
          "messagingSenderId": "995180480938"
        },
        "sentryDsn": "${SENTRY_DSN}",
        "gaTrackingId": "${GA_TRACKINGID}"
      }
    }
  }
}
